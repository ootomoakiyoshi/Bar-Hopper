<table class="table">
  <thead>
    <tr>
      <th scope="col"></th>
      <th scope="col">Name</th>
      <th scope="col"></th>
      <th scope="col"></th>
    </tr>
    <% users.each do |user| %>
    <tr>
     <td><%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %></td>
     <td> <%= link_to user.name, user_path(user) %></td>
     <% end %>
  </thead>
</table>